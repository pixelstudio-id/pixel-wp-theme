{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": false,
    "border": {
      "color": false,
      "radius": false,
      "style": false,
      "width": false
    },
    "color": {
      "background": true,
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "defaultGradients": false,
      "defaultPalette": false,
      "duotone": [],
      "link": false,
      "text": true,
      "palette": [
        { "name": "Primary",       "slug": "color-1",       "color": "var(--color1)" },
        { "name": "Primary Light", "slug": "color-1-light", "color": "var(--color1Light)" },

        { "name": "Secondary",       "slug": "color-2",       "color": "var(--color2)" },
        { "name": "Secondary Light", "slug": "color-2-light", "color": "var(--color2Light)" },

        { "name": "Tertiary",       "slug": "color-3",       "color": "var(--color3)" },
        { "name": "Tertiary Light", "slug": "color-3-light", "color": "var(--color3Light)" },

        { "name": "Base",        "slug": "base",   "color": "var(--base)" },
        { "name": "Base Inverse", "slug": "base-inv", "color": "var(--baseInv)" },
        { "name": "Gray",       "slug": "gray",       "color": "var(--gray)" },
        { "name": "Gray Dark",  "slug": "gray-dark",  "color": "var(--grayDark)" },
        { "name": "Gray Light", "slug": "gray-light", "color": "var(--grayLight)" }
      ],
      "gradients": [
        {
          "slug": "base-to-bottom",
          "gradient": "linear-gradient(to bottom, var(--base), transparent)",
          "name": "Black to Bottom"
        },
        {
          "slug": "base-to-top",
          "gradient": "linear-gradient(to top, var(--base), transparent)",
          "name": "Black to Top"
        },
        {
          "slug": "base-inverse-to-bottom",
          "gradient": "linear-gradient(to bottom, var(--baseInv), transparent)",
          "name": "White to Bottom"
        },
        {
          "slug": "base-inverse-to-top",
          "gradient": "linear-gradient(to top, var(--baseInv), transparent)",
          "name": "White to Top"
        },
        {
          "slug": "color-1-to-bottom",
          "gradient": "linear-gradient(to bottom, var(--color1), transparent)",
          "name": "Color 1 to Bottom"
        },
        {
          "slug": "color-1-to-top",
          "gradient": "linear-gradient(to top, var(--color1), transparent)",
          "name": "Color 1 to Top"
        },
        {
          "slug": "color-1-light-to-bottom",
          "gradient": "linear-gradient(to bottom, var(--color1Light), transparent)",
          "name": "Color 1 Light to Bottom"
        },
        {
          "slug": "color-1-light-to-top",
          "gradient": "linear-gradient(to top, var(--color1Light), transparent)",
          "name": "Color 1 Light to Top"
        },
        {
          "slug": "color-2-to-bottom",
          "gradient": "linear-gradient(to bottom, var(--color2), transparent)",
          "name": "Color 2 to Bottom"
        },
        {
          "slug": "color-2-to-top",
          "gradient": "linear-gradient(to top, var(--color2), transparent)",
          "name": "Color 2 to Top"
        },
        {
          "slug": "color-2-light-to-bottom",
          "gradient": "linear-gradient(to bottom, var(--color2Light), transparent)",
          "name": "Color 2 Light to Bottom"
        },
        {
          "slug": "color-2-light-to-top",
          "gradient": "linear-gradient(to top, var(--color2Light), transparent)",
          "name": "Color 2 Light to Top"
        }
      ]
    },
    "custom": {},
    "layout": {
      "contentSize": "650px",
      "wideSize": "1120px"
    },
    "spacing": {
      "blockGap": false,
      "padding": true,
			"margin": true,
      "spacingScale": {
				"steps": 1
			},
			"spacingSizes": [
				{
					"size": "var(--space1)",
					"slug": "space1",
					"name": "1"
				},
				{
					"size": "var(--space2)",
					"slug": "space2",
					"name": "2"
				},
				{
					"size": "var(--space3)",
					"slug": "space3",
					"name": "3"
				},
				{
					"size": "var(--space4)",
					"slug": "space4",
					"name": "4"
				},
				{
					"size": "var(--space5)",
					"slug": "space5",
					"name": "5"
				},
				{
					"size": "var(--space6)",
					"slug": "space6",
					"name": "6"
				}
			],
      "units": [ "px", "%", "rem", "vh", "vw" ]
    },
    "useRootPaddingAwareAlignments": true,
    "typography": {
      "customFontSize": true,
      "dropCap": true,
      "fontFamilies": [],
      "fontSizes": [
        { "name": "Small",   "size": "var(--sFontSize)", "slug": "small" },
        { "name": "Regular", "size": "var(--fontSize)",  "slug": "regular" },
        { "name": "Medium",  "size": "var(--mFontSize)", "slug": "medium" },
        { "name": "Large",   "size": "var(--lFontSize)", "slug": "large" },
        { "name": "Huge",    "size": "var(--xlFontSize)", "slug": "huge" }
      ],
      "fontStyle": false,
      "fontWeight": false,
      "letterSpacing": false,
      "lineHeight": false,
      "textDecoration": false,
      "textTransform": false
    },
    "blocks": {}
  }
}